{
  "name": "reps-archive",
  "version": "0.0.1",
  "license": "MPL",
  "author": "Michael Kohler <me@michaelkohler.info>",
  "scripts": {
    "clean": "rm -rf dist/index.html dist/events dist/reps",
    "start": "npm run clean && DEBUG=reps-archive:* node index.js",
    "eslint": "eslint .",
    "test": "nyc ava",
    "deploy": "npm run start && gh-pages -d dist"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.19.2",
    "debug": "^4.2.0",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@michaelkohler/eslint-config-base": "^6.8.20",
    "@michaelkohler/eslint-config-node": "^6.8.20",
    "@michaelkohler/eslint-config-test": "^6.8.20",
    "ava": "^3.5.1",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "mocked-env": "^1.3.2",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "sinon": "^9.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 90,
    "branches": 85,
    "functions": 90,
    "lines": 90,
    "include": [
      "gather.js",
      "lib/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "timeout": "10s",
    "babel": {
      "compileAsTests": [
        "tests/**/*.js"
      ]
    }
  }
}
